{
  "name": "@code-to-json/schema",
  "version": "0.0.20",
  "description": "",
  "main": "lib/src/index.js",
  "typings": "lib/src/index.d.ts",
  "license": "BSD-2-Clause",
  "author": "Mike North <michael.l.north@gmail.com> (https://mike.works)",
  "scripts": {
    "lint": "tslint --project .",
    "test": "mocha",
    "test:coverage": "nyc npm test",
    "clean": "rm -rf ./lib",
    "build": "tsc -b .",
    "rebuild": "npm run clean & npm run build",
    "prepublishOnly": "tsc -b ."
  },
  "repository": "https://github.com/mike-north/code-to-json/tree/master/packages/schema",
  "keywords": [],
  "devDependencies": {
    "@code-to-json/test-helpers": "^0.0.20",
    "nyc": "13.1.0",
    "snap-shot-it": "6.2.3",
    "tslint": "5.11.0",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "@microsoft/api-extractor": "^6.0.8",
    "strip-json-comments": "^2.0.1"
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    },
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "reporter": [
      "lcov",
      "json",
      "text-summary"
    ]
  }
}
