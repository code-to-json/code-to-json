{
  "name": "@code-to-json/core-linker",
  "version": "1.0.0-rc.48",
  "description": "Data linker for @code-to-json/core",
  "main": "lib/src/index.js",
  "typings": "lib/src/index.d.ts",
  "license": "BSD-2-Clause",
  "author": "Mike North <michael.l.north@gmail.com> (https://mike.works)",
  "scripts": {
    "lint": "yarn lint:js && yarn lint:md",
    "lint:js": "eslint . --ext js,ts",
    "lint:md": "./node_modules/.bin/remark .",
    "test": "mocha test/**/*.test.ts",
    "test:debug": "yarn test --inspect-brk",
    "test:coverage": "nyc yarn test",
    "codacy:transmit-coverage": "cat coverage/lcov.info | codacy-coverage",
    "clean": "rm -rf ./lib",
    "build": "tsc -b .",
    "rebuild": "yarn clean & yarn build",
    "prepublishOnly": "tsc -b ."
  },
  "repository": "https://github.com/mike-north/code-to-json/",
  "homepage": "https://github.com/mike-north/code-to-json/tree/master/packages/core-linker",
  "keywords": [],
  "dependencies": {
    "@code-to-json/core": "^1.0.0-rc.57",
    "@mike-north/types": "^1.0.7",
    "debug": "^4.0.0",
    "typescript": "^3.4.3"
  },
  "devDependencies": {
    "@code-to-json/test-helpers": "^1.0.0-rc.48",
    "@code-to-json/utils-node": "^1.0.0-rc.52",
    "@types/chai": "4.3.4",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "chai": "4.3.7",
    "eslint": "8.36.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.0.0",
    "mocha": "10.0.0",
    "nyc": "15.1.0",
    "remark-cli": "10.0.1",
    "remark-lint": "9.1.1",
    "remark-lint-no-dead-urls": "1.1.0",
    "remark-preset-lint-markdown-style-guide": "5.1.2",
    "remark-preset-lint-recommended": "6.1.2",
    "remark-validate-links": "11.0.2",
    "ts-node": "10.7.0"
  },
  "nyc": {
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    },
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "reporter": [
      "lcov",
      "json",
      "text-summary"
    ]
  }
}
