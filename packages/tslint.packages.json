{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-sonarts"],
  "jsRules": {
    "quotemark": false,
    "whitespace": false,
    "variable-name": [true, "allow-leading-underscore"],
    "trailing-comma": true,
    "object-literal-sort-keys": false,
    "no-debugger": false,
    "no-bitwise": false,
    "no-console": false
  },
  "rules": {
    "quotemark": false,
    "whitespace": false,
    "variable-name": [true, "allow-leading-underscore"],
    "trailing-comma": true,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "no-debugger": false,
    "typedef": [true, "call-signature"],
    "prefer-immediate-return": false,
    "no-bitwise": false,
    "no-console": false,
    "max-union-size": [true, 9],
    "mccabe-complexity": [true, 20],
    "bool-param-default": true,
    "no-small-switch": false,
    "ordered-imports": false,
    "parameters-max-number": [true, 5],
    "prefer-type-guard": true,
    "use-primitive-type": true,
    "max-classes-per-file": false,
    "no-duplicate-string": false,
    "max-line-length": false
  },
  "rulesDirectory": []
}
